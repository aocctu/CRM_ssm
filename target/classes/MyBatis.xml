<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC">
				<property name="" value="" />
			</transactionManager>
			<dataSource type="UNPOOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost:3306/erp" />
				<property name="username" value="root" />
				<property name="password" value="123" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="com/examsys/mapper/AdminMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/AdminRolesSettingsMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/AdminRolesMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/DepartmentMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/JobInfoMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/EmployeeMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/RightsMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/JobRightMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/CustomMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ConsultRecordMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/CustomInfoMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ResetpassMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ExpressMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ExpressCompanyMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ExpressSortMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/PartsWarehouseMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/DeliveryMapper.xml"></mapper>
		<mapper resource="com/examsys/mapper/ExpressSendMapper.xml"></mapper>
		
	</mappers>
	
	<!-- 配置分页插件 -->
