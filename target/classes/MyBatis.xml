		<plugin interceptor="com.github.pagehelper.PageInterceptor">
			设置数据库类型 Oracle,Mysql,MariaDB,SQLite,Hsqldb,PostgreSQL六种数据库        
        	<property name="dialect" value="Oracle"/>
		</plugin>
	</plugins> -->
</configuration> 